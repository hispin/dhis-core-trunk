			<result name="success" type="velocity-json">/dhis-web-mapping/void.vm
			</result>
		</action>

		<action name="addOrUpdateMapOrganisationUnitRelations"
			class="org.hisp.dhis.mapping.action.AddOrUpdateMapOrganisationUnitRelationsAction">
			<result name="success" type="velocity-json">/dhis-web-mapping/void.vm
