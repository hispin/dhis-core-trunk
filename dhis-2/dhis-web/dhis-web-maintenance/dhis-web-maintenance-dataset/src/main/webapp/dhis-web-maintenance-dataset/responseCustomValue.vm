<?xml version="1.0" encoding="UTF-8"?>
#set($count = 0)
<customValue>
  <customValues>
    #foreach( $customValueId in $customValueIds )
      #set($customValueName = $customValueNames.get($count))    
      <customValue id="$customValueId">$encoder.xmlEncode( $customValueName )</customValue>
      #set($count = $count + 1)
    #end
  </customValues>
</customValue>