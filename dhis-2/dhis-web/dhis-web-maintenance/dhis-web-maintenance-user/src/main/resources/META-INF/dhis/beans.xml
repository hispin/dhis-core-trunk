		scope="prototype">
		<property name="dataSetService">
			<ref bean="org.hisp.dhis.dataset.DataSetService" />
		</property>
		<property name="authoritiesProvider">
			<ref bean="org.hisp.dhis.security.authority.SystemAuthoritiesProvider" />
		</property>
		<property name="reportExcelService">
			<ref bean="org.hisp.dhis.reportexcel.ReportExcelService" />
		</property>
	</bean>

	<bean id="org.hisp.dhis.user.action.ValidateRoleAction" class="org.hisp.dhis.user.action.ValidateRoleAction"
		scope="prototype">
		<property name="userStore">
			<ref bean="org.hisp.dhis.user.UserStore" />
		</property>
	</bean>

</beans>
