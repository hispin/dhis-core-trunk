<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<include file="dhis-web-commons.xml" />

	<package name="dhis-web-excel-reporting" extends="dhis-web-commons"
		namespace="/dhis-web-excel-reporting">

		<action name="administration" class="org.hisp.dhis.reportexcel.action.NoAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/viewAdministrator.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<!-- SYSTEM CONFIGURATION ACTION -->

		<action name="getConfiguration"
			class="org.hisp.dhis.reportexcel.configuration.action.GetReportConfigurationAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/configuration.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="setConfiguration"
			class="org.hisp.dhis.reportexcel.configuration.action.SetReportConfigurationAction">
			<result name="success" type="redirect">administration.action
			</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<!-- REPORT EXCEL CORE ACTION -->

		<action name="index" class="org.hisp.dhis.reportexcel.action.NoAction">
			<result name="success" type="redirect">listReportExcel.action
			</result>
		</action>

		<action name="addReportExcel"
			class="org.hisp.dhis.reportexcel.action.AddReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReport.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="deleteReportExcel"
			class="org.hisp.dhis.reportexcel.action.DeleteReportExcelAction">
			<result name="success" type="redirect">listReportExcel.action
			</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="getReportExcel"
			class="org.hisp.dhis.reportexcel.action.GetReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReport.vm</result>
		</action>

		<action name="getReportExcelGroups"
			class="org.hisp.dhis.reportexcel.action.GetReportExcelGroupsAction">
			<result name="success" type="velocity">
				/dhis-web-excel-reporting/responseReportGroups.vm</result>
		</action>

		<action name="listReportExcel"
			class="org.hisp.dhis.reportexcel.action.ListReportExcelAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/reports.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="javascripts">javascript/reports.js,javascript/jquery.autocomplete.js,javascript/commons.js</param>
			<param name="stylesheets">style/jquery.autocomplete.css</param>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="updateReportExcel"
			class="org.hisp.dhis.reportexcel.action.UpdateReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReport.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="validateAddReportExcel"
			class="org.hisp.dhis.reportexcel.action.ValidateAddReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>

		<action name="validateUpdateReportExcel"
			class="org.hisp.dhis.reportexcel.action.ValidateUpdateReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>

		<!-- REPORT EXCEL CATEGORY ACTION -->

		<action name="listDataElementGroupOrder"
			class="org.hisp.dhis.reportexcel.action.GetReportExcelAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/listDataElementGroup.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="javascripts">javascript/dataElementGroup.js,javascript/commons.js</param>
			<param name="stylesheets">style/basic.css</param>
		</action>

		<action name="getAllDataElementGroups"
			class="org.hisp.dhis.reportexcel.category.action.GetAllDataElementGroupsAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseDataElementGroups.vm</result>
		</action>

		<action name="addDataElementGroupOrder"
			class="org.hisp.dhis.reportexcel.category.action.SaveDataElementGroupOrderAction">
			<result name="success" type="redirect">
				listDataElementGroupOrder.action?id=${id}</result>
		</action>

		<action name="updateDataElementGroupOrder"
			class="org.hisp.dhis.reportexcel.category.action.UpdateDataElementGroupOrderAction">
			<result name="success" type="redirect">
				listDataElementGroupOrder.action?id=${id}</result>
		</action>

		<action name="deleteDataElementGroupOrder"
			class="org.hisp.dhis.reportexcel.category.action.DeleteDataElementGroupOrderAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
		</action>

		<action name="getDataElementGroupOrder"
			class="org.hisp.dhis.reportexcel.category.action.GetDataElementGroupOrderAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseDataElementOrder.vm</result>
		</action>

		<action name="openSortDataElement"
			class="org.hisp.dhis.reportexcel.category.action.GetDataElementGroupOrderAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/sortDataElements.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="javascripts">javascript/ui.core.js,javascript/ui.core.js,javascript/ui.sortable.js,javascript/dataElementGroup.js,javascript/commons.js</param>
			<param name="stylesheets">style/ui.theme.css,style/basic.css</param>
		</action>

		<action name="updateSortedDataElements"
			class="org.hisp.dhis.reportexcel.category.action.UpdateSortedDataElementAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
		</action>

		<!-- REPORT EXCEL ITEM ACTION -->

		<action name="listReportExcelItemAction"
			class="org.hisp.dhis.reportexcel.item.action.ListReportExcelItemAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/reportItems.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menu.vm</param>
			<param name="javascripts">javascript/reportItem.js,javascript/dataelement.js,javascript/jquery.autocomplete.js,javascript/commons.js</param>
			<param name="stylesheets">style/jquery.autocomplete.css</param>
		</action>

		<action name="getReportExcelItems"
			class="org.hisp.dhis.reportexcel.item.action.ListReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReportItems.vm</result>
		</action>

		<action name="getReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.GetReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReportItem.vm</result>
		</action>

		<action name="validateAddReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.ValidateAddReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>

		<action name="validateUpdateReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.ValidateUpdateReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>

		<action name="addReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.AddReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReportItem.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="updateReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.UpdateReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseReportItem.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="deleteReportExcelItem"
			class="org.hisp.dhis.reportexcel.item.action.DeleteReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="getOptionCombos"
			class="org.hisp.dhis.reportexcel.item.action.GetOptionCombosAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseOptionCombo.vm</result>
		</action>

		<action name="getAllReportExcels"
			class="org.hisp.dhis.reportexcel.action.ListReportExcelAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseListReport.vm</result>
		</action>

		<action name="copyReportExcelItems"
			class="org.hisp.dhis.reportexcel.item.action.CopyReportExcelItemAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<param name="requiredAuthorities">F_EXCEL_REPORT_ADMINISTRATION</param>
		</action>

		<action name="getListSheet"
			class="org.hisp.dhis.reportexcel.item.action.GetListSheetAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/sheets.vm</result>
		</action>

		<action name="getDataElementOrder"
			class="org.hisp.dhis.reportexcel.category.action.GetDataElementOrderAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseDataElementOrder.vm</result>
		</action>


		<!-- IMPORT EXCEL FILES -->

		<action name="getInformation"
			class="org.hisp.dhis.reportexcel.importing.action.UploadExcelFileAction">
			<result name="success" type="chain">
				selectFormChain</result>
			<interceptor-ref name="fileUploadStack" />
		</action>

		<action name="selectFormChain"
			class="org.hisp.dhis.reportexcel.export.action.SelectFormAction">
			<result name="success" type="chain">getInformationChain
			</result>
		</action>

		<action name="getInformationChain"
			class="org.hisp.dhis.reportexcel.importing.action.GetInformationAction">
			<interceptor-ref name="organisationUnitTreeStack" />
			<result name="success" type="velocity">/main.vm</result>
			<param name="page">/dhis-web-excel-reporting/importData.vm</param>
			<param name="menu">/dhis-web-excel-reporting/menuWithTree.vm</param>
			<param name="menuTreeHeight">220</param>
			<param name="javascripts">../dhis-web-commons/ouwt/ouwt.js,javascript/imports.js,
				javascript/export.js</param>
			<param name="stylesheets">style/style.css</param>
		</action>

		<action name="importData"
			class="oorg.hisp.dhis.reportexcel.importing.action.ImportDataAction">
			<result name="success" type="velocity-xml">
				/dhis-web-excel-reporting/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>

		<action name="previewData"
			class="org.hisp.dhis.reportexcel.importing.action.ViewDataAction">
			<result name="success" type="velocity">
				/dhis-web-excel-reporting/responseReportItemValues.vm</result>
			<result name="error" type="velocity-xml">
				/dhis-web-excel-reporting/responseError.vm</result>
		</action>


	</package>
</struts>
