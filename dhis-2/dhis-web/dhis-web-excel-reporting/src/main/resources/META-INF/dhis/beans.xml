	<bean
		id="org.hisp.dhis.reportexcel.action.OpenReportExcelAssociationsAction"
		class="org.hisp.dhis.reportexcel.action.OpenReportExcelAssociationsAction"
		scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
		<property name="selectionTreeManager"
			ref="org.hisp.dhis.oust.manager.SelectionTreeManager" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.action.UpdateReportAssociationsAction"
		class="org.hisp.dhis.reportexcel.action.UpdateReportAssociationsAction"
		scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
		<property name="selectionTreeManager"
			ref="org.hisp.dhis.oust.manager.SelectionTreeManager" />
	</bean>

	<bean
		id="org.hisp.dhis.reportexcel.association.action.GetNumberOfLevelsAction"
		class="org.hisp.dhis.reportexcel.association.action.GetNumberOfLevelsAction"
