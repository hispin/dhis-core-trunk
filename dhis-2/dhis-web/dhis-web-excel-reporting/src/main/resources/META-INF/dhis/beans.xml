			<ref bean="org.hisp.dhis.dataelement.DataElementService" />
		</property>
		<property name="dataElementCategoryOptionComboService">
			<ref
				bean="org.hisp.dhis.dataelement.DataElementCategoryOptionComboService" />
		</property>
	</bean>

	<bean id="org.hisp.dhis.reportexcel.item.action.CopyReportExcelItemAction"
		class="org.hisp.dhis.reportexcel.item.action.CopyReportExcelItemAction"
		scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
	</bean>

