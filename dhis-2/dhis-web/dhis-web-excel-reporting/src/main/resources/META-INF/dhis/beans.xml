<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="org.hisp.dhis.reportexcel.export.action.SelectionManager"
		class="org.hisp.dhis.reportexcel.export.action.SelectionManager"
		scope="singleton">
	</bean>

	<!-- SYSTEM CONFIGURATION BEAN -->

	<bean
		id="org.hisp.dhis.reportexcel.configuration.action.GetReportConfigurationAction"
		class="org.hisp.dhis.reportexcel.configuration.action.GetReportConfigurationAction"
		scope="prototype">
		<property name="systemSettingManager" ref="org.hisp.dhis.options.SystemSettingManager" />
	</bean>

	<bean
		id="org.hisp.dhis.reportexcel.configuration.action.SetReportConfigurationAction"
		class="org.hisp.dhis.reportexcel.configuration.action.SetReportConfigurationAction"
		scope="prototype">
		<property name="systemSettingManager" ref="org.hisp.dhis.options.SystemSettingManager" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.configuration.action.GetAdminOptionsAction"
		class="org.hisp.dhis.reportexcel.configuration.action.GetAdminOptionsAction" scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
		<property name="userStore" ref="org.hisp.dhis.user.UserStore" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.configuration.action.SaveReportUserRolesAction"
		class="org.hisp.dhis.reportexcel.configuration.action.SaveReportUserRolesAction" scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
		<property name="userStore" ref="org.hisp.dhis.user.UserStore" />
	</bean>
	
	<!-- REPORT EXCEL CORE BEAN -->

	<bean id="org.hisp.dhis.reportexcel.action.AddReportExcelAction"
		class="org.hisp.dhis.reportexcel.action.AddReportExcelAction" scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.action.DeleteReportExcelAction"
		class="org.hisp.dhis.reportexcel.action.DeleteReportExcelAction"
		scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.action.GetReportExcelAction"
		class="org.hisp.dhis.reportexcel.action.GetReportExcelAction" scope="prototype">
		<property name="reportService" ref="org.hisp.dhis.reportexcel.ReportExcelService" />
	</bean>

	<bean id="org.hisp.dhis.reportexcel.action.GetReportExcelGroupsAction"
		class="org.hisp.dhis.reportexcel.action.GetReportExcelGroupsAction"
