<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.dataentryform.DataEntryFormAssociation" table="dataentryformassociation">
  
   	<composite-id>
          
      <key-property name="associationTableName" column="associationtablename" length="50"/>
      
      <key-property name="associationId" column="associationid" />
          
    </composite-id>
    
 	<many-to-one name="dataEntryForm" class="org.hisp.dhis.dataentryform.DataEntryForm"
       column="dataentryformid" foreign-key="fk_dataentryformassociation_dataentryformid" lazy="false"/>
    
  </class>
</hibernate-mapping>
