<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

  <bean id="org.hisp.dhis.options.sortorder.SortOrderManager" class="org.hisp.dhis.options.sortorder.DefaultSortOrderManager"
    init-method="init">
    <property name="userSettingService" ref="org.hisp.dhis.user.UserSettingService" />
  </bean>

  <bean id="org.hisp.dhis.options.displayproperty.DisplayPropertyManager" class="org.hisp.dhis.options.displayproperty.DefaultDisplayPropertyManager">
    <property name="userSettingService" ref="org.hisp.dhis.user.UserSettingService" />
  </bean>

  <bean id="org.hisp.dhis.options.style.StyleManager" class="org.hisp.dhis.options.style.DefaultStyleManager">
    <property name="systemSettingManager" ref="org.hisp.dhis.options.SystemSettingManager" />
	<property name="userSettingService" ref="org.hisp.dhis.user.UserSettingService" />
    <property name="defaultStyle" value="light_blue" />
    <property name="styles">
      <map>
        <entry>
          <key>
            <value>light_blue</value>
          </key>
          <value>light_blue/light_blue.css</value>
        </entry>
        <entry>
          <key>
            <value>blue</value>
          </key>
          <value>blue/blue.css</value>
        </entry>
        <entry>
          <key>
            <value>green</value>
          </key>
          <value>green/green.css</value>
        </entry>
        <entry>
          <key>
            <value>vietnam</value>
          </key>
          <value>vietnam/vietnam.css</value>
        </entry>
        <entry>
          <key>
            <value>india</value>
          </key>
          <value>india/india.css</value>
        </entry>
      </map>
    </property>
  </bean>

  <bean id="org.hisp.dhis.options.help.HelpManager" class="org.hisp.dhis.options.help.DefaultHelpManager" />

</beans>
