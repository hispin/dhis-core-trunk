<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.reporttable.ReportTableColumn" table="reporttablecolumn">
  
    <id name="id" column="reporttablecolumnid">
      <generator class="native"/>
    </id>
    
    <property name="name"/>
    <property name="header"/>
    <property name="hidden"/>
    
  </class>
</hibernate-mapping>
