<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="org.hisp.dhis.reportexcel.chart.ExtBookmarkChart"
		table="extbookmarkcharts">

		<id name="id" column="chartid">
			<generator class="native" />
		</id>

		<property name="name" column="name" not-null="true" unique="true"/>

		<property name="title" column="title" not-null="true" />
		
		<property name="xtitle" column="xtitle" not-null="true" />		

		<property name="username" column="username" not-null="true" />
		
		<property name="description" />

		<property name="jsonValueStore" column="json" not-null="true" length="10000000"/>


	</class>

</hibernate-mapping>
