<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="org.hisp.dhis.mapping.Map" table="map">
    
    	<id name="id" column="mapid">
			<generator class="native"/>
		</id>
			
		<property name="name" column="name" unique="true"/>
	
		<property name="mapLayerPath" column="maplayerpath"/>
		
		<property name="type" column="type"/>
			
		<many-to-one name="organisationUnit" class="org.hisp.dhis.organisationunit.OrganisationUnit" 
			column="organisationunitid" not-null="true" foreign-key="fk_map_organisationunitid"/>
		  
		<many-to-one name="organisationUnitLevel" class="org.hisp.dhis.organisationunit.OrganisationUnitLevel" 
			column="organisationunitlevelid" not-null="true" foreign-key="fk_map_organisationunitlevelid"/>
			
		<property name="uniqueColumn" column="uniquecolumn"/>
		
		<property name="nameColumn" column="namecolumn"/>
			
		<property name="longitude" column="longitude"/>
			
		<property name="latitude" column="latitude"/>
			
		<property name="zoom" column="zoom"/>
	
	</class>
	
</hibernate-mapping>