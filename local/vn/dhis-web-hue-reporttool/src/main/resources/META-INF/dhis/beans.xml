<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- DataAccess -->
	
	<bean id="org.hisp.dhis.rt.dataaccess.ReportDataAccess" class="org.hisp.dhis.rt.dataaccess.Dhis20ReportDataAccess" scope="prototype">
		<property name="organisationUnitService">
			<ref bean="org.hisp.dhis.organisationunit.OrganisationUnitService"/>
    </property>
		<property name="indicatorStore">
			<ref bean="org.hisp.dhis.indicator.IndicatorStore"/>
    </property>
		<property name="dataElementService">
			<ref bean="org.hisp.dhis.dataelement.DataElementService"/>
    </property>
		<property name="aggregationService">
			<ref bean="org.hisp.dhis.aggregation.AggregationService"/>
		</property>
	</bean>
	
	<!-- ReportStore -->
	
	<bean id="org.hisp.dhis.rt.report.ReportStore" class="org.hisp.dhis.rt.report.XmlReportStore" scope="prototype">
		<property name="reportDataAccess">
			<ref bean="org.hisp.dhis.rt.dataaccess.ReportDataAccess"/>
		</property>
		<property name="reportDisplayProperty">
			<value>name</value>
		</property>
	</bean>
	
	<!-- Action -->
	
	<bean id="org.hisp.dhis.rt.action.ViewReportAction" class="org.hisp.dhis.rt.action.ViewReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="reportDataAccess">
			<ref bean="org.hisp.dhis.rt.dataaccess.ReportDataAccess"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.AddDataElementToReportAction" class="org.hisp.dhis.rt.action.AddDataElementToReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="selectionManager">
			<ref bean="org.hisp.dhis.ouwt.manager.OrganisationUnitSelectionManager"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.AddDataElementToChartAction" class="org.hisp.dhis.rt.action.AddDataElementToChartAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="selectionManager">
			<ref bean="org.hisp.dhis.ouwt.manager.OrganisationUnitSelectionManager"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.AddIndicatorToReportAction" class="org.hisp.dhis.rt.action.AddIndicatorToReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="selectionManager">
			<ref bean="org.hisp.dhis.ouwt.manager.OrganisationUnitSelectionManager"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.AddIndicatorToChartAction" class="org.hisp.dhis.rt.action.AddIndicatorToChartAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="selectionManager">
			<ref bean="org.hisp.dhis.ouwt.manager.OrganisationUnitSelectionManager"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.SetDesignTemplateAction" class="org.hisp.dhis.rt.action.SetDesignTemplateAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.SetChartTemplateAction" class="org.hisp.dhis.rt.action.SetChartTemplateAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.AddReportAction" class="org.hisp.dhis.rt.action.AddReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.DeleteReportAction" class="org.hisp.dhis.rt.action.DeleteReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.LoadReportAction" class="org.hisp.dhis.rt.action.LoadReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.UnloadReportAction" class="org.hisp.dhis.rt.action.UnloadReportAction" scope="prototype"/>
	
	<bean id="org.hisp.dhis.rt.action.PreviewReportAction" class="org.hisp.dhis.rt.action.PreviewReportAction" scope="prototype"/>
	
	<bean id="org.hisp.dhis.rt.action.RemoveReportElementAction" class="org.hisp.dhis.rt.action.RemoveReportElementAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.RemoveChartElementAction" class="org.hisp.dhis.rt.action.RemoveChartElementAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.ListReportsAction" class="org.hisp.dhis.rt.action.ListReportsAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.ListDesignsAction" class="org.hisp.dhis.rt.action.ListDesignsAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.MoveUpReportElementAction" class="org.hisp.dhis.rt.action.MoveUpReportElementAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.MoveDownReportElementAction" class="org.hisp.dhis.rt.action.MoveDownReportElementAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.GenerateDesignAction" class="org.hisp.dhis.rt.action.GenerateDesignAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.GenerateReportAction" class="org.hisp.dhis.rt.action.GenerateReportAction" scope="prototype">
		<property name="reportStore">
			<ref bean="org.hisp.dhis.rt.report.ReportStore"/>
		</property>
		<property name="reportDataAccess">
			<ref bean="org.hisp.dhis.rt.dataaccess.ReportDataAccess"/>
		</property>		
		<property name="selectionManager">
			<ref bean="org.hisp.dhis.ouwt.manager.OrganisationUnitSelectionManager"/>
		</property>
	</bean>
	
	<bean id="org.hisp.dhis.rt.action.NoAction" class="org.hisp.dhis.rt.action.NoAction" />
	
</beans>
