<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.hisp.dhis.vn.report.DataElementOrderInGroup"
		table="dataelementorder">
		<id name="id" column="dataelementorderingroupid">
			<generator class="native" />
		</id>
		<property name="dataElementGroupId" column="datalementgroupid" />
		<property name="dataElementGroupName" column="dataelementgroupname" />
		<list name="dataElements" table="dataelementorder_dataElement" lazy="false">
			<key column="dataelementorderid" />
			<list-index base="0" column="dataelementorder" />
			<many-to-many class="org.hisp.dhis.dataelement.DataElement"
				column="dataelementid" foreign-key="fk_dataelementorderid_dataelementid" />
		</list>
	</class>
</hibernate-mapping>