			<!-- Form Action -->
		<!--  --><!--  --><!--  -->
		
		<!-- List Form -->
		<action name="listForm" class="org.hisp.dhis.vn.chr.form.action.ListFormAction">
			<result name="success" type="chain">getVisibleForms</result>
			<param name="page">/dhis-web-chr-form/forms.vm</param>
		</action>
			
		<!--  Add Form  -->
		<action name="addForm" class="org.hisp.dhis.vn.chr.form.action.AddFormAction">
			<result name="success" type="velocity-xml">/dhis-web-chr-form/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">/dhis-web-chr-form/responseError.vm</result>
		</action>
		
		<!--  Update Form  -->
		<action name="updateForm" class="org.hisp.dhis.vn.chr.form.action.UpdateFormAction">
