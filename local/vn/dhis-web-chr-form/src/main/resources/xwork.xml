		<action name="getVisibleForms" class="org.hisp.dhis.vn.chr.form.action.GetVisibleFormsAction">
			<result name="success" type="velocity">/main.vm</result>
			<param name="menu">/dhis-web-chr-form/menu.vm</param>
			<param name="javascripts">javascripts/forms.js,javascripts/dhtmlgoodies_calendar.js</param>
		</action>
		
		<!--  Varidate Form  -->
		<action name="validateForm" class="org.hisp.dhis.vn.chr.form.action.ValidateFormAction">
			<result name="success" type="velocity">/dhis-web-chr-form/responseSuccess.vm</result>
			<result name="error" type="velocity-xml">/dhis-web-chr-form/responseError.vm</result>
		</action>
		
