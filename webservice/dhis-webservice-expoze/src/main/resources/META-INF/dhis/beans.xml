<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="org.hisp.dhis.webservice.adapter.period.PeriodServiceAdapter"
    class="org.hisp.dhis.webservice.adapter.period.DefaultPeriodServiceAdapter">
    <property name="periodService" ref="org.hisp.dhis.period.PeriodService"/>
  </bean>
  
  <bean id="org.hisp.dhis.webservice.adapter.indicator.IndicatorServiceAdapter"
    class="org.hisp.dhis.webservice.adapter.indicator.DefaultIndicatorServiceAdapter">
    <property name="indicatorService" ref="org.hisp.dhis.indicator.IndicatorService"/>
  </bean>
    
  <bean id="org.hisp.dhis.webservice.adapter.organisationunit.OrganisationUnitServiceAdapter"
    class="org.hisp.dhis.webservice.adapter.organisationunit.DefaultOrganisationUnitServiceAdapter">
    <property name="organisationUnitService" ref="org.hisp.dhis.organisationunit.OrganisationUnitService"/>
  </bean>
  
</beans>